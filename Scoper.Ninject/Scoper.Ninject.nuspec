      <dependency id="Ninject" version="[3.2.0.0,)" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\Scoper.Ninject.dll" target="lib\net45" />
    <file src="bin\Release\Scoper.dll" target="lib\net45" />
  </files>
</package>